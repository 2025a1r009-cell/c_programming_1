#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char ch;
    scanf("%c", &ch);
    if (ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u'||ch=='A'||ch=='I'||ch=='E'||ch=='O'||ch == 'U')
        printf("Vowel");
    else if ((ch>=65 && ch<= 90)||(ch>=97 && ch<=122))
        printf("Consonant");
    else
        printf("Invalid Input");
    return 0;
}
