#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>


int main() {
    int amount;
    int n500, n100, n50, n20, n10, n5, n2, n1;

    // Input amount
    scanf("%d", &amount);

    // Calculate number of notes for each denomination
    n500 = amount / 500; amount %= 500;
    n100 = amount / 100; amount %= 100;
    n50  = amount / 50;  amount %= 50;
    n20  = amount / 20;  amount %= 20;
    n10  = amount / 10;  amount %= 10;
    n5   = amount / 5;   amount %= 5;
    n2   = amount / 2;   amount %= 2;
    n1   = amount;

    // Print only denominations that are used
    if (n500 > 0) printf("500: %d\n", n500);
    if (n100 > 0) printf("100: %d\n", n100);
    if (n50  > 0) printf("50: %d\n", n50);
    if (n20  > 0) printf("20: %d\n", n20);
    if (n10  > 0) printf("10: %d\n", n10);
    if (n5   > 0) printf("5: %d\n", n5);
    if (n2   > 0) printf("2: %d\n", n2);
    if (n1   > 0) printf("1: %d\n", n1);

    return 0;
}
